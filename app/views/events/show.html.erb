<div class="events-background-color">
<%= render 'layouts/navbar' %>

  <div class="imagestockpile">
      <div id="slider" class="carousel carousel-slide" data-transition="slide">
        <% @photo_detail.each do |photo| %>
          <div class="carousel-item"><img src="<%= photo["url"] %>"/>
            <div class="photo-title"><h3><%= photo["title"] %></h3>
            </div>
          </div>
      <% end %>
    </div>
  </div>
  <button id="add_img_gratitude" class= "btn btn-default center-block">Add this as a gratitude</button>
</div>
